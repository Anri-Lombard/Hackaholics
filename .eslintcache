ing\n  const submitOrderHandler = async (userData) => {\n    setIsSubmittingState(true);\n\n    const response = await fetch(\n      \"https://react-http-7bd76-default-rtdb.firebaseio.com/orders.json\",\n      {\n        method: \"POST\",\n        body: JSON.stringify({\n          user: userData,\n          orderedItems: cartCtx.items,\n        }),\n      }\n    );\n    setIsSubmittingState(false);\n    setDidSubmit(true);\n  };\n\n  const cartItems = (\n    <ul className={classes[\"cart-items\"]}>\n      {cartCtx.items.map((item) => (\n        <CartItem\n          key={item.id}\n          name={item.name}\n          amount={item.amount}\n          price={item.date}\n          onRemove={cartItemRemoveHandler.bind(null, item.id)}\n          onAdd={cartItemAddHandler.bind(null, item)}\n        />\n      ))}\n    </ul>\n  );\n\n  const modalActions = (\n    <div className={classes.actions}>\n      <button className={classes[\"button--alt\"]} onClick={props.onClose}>\n        Close\n      </button>\n      {hasItems && (\n        <button onClick={orderHandler} className={classes.button}>\n          Order\n        </button>\n      )}\n    </div>\n  );\n\n  const cartModalContent = (\n    <React.Fragment>\n      {cartItems}\n      <div className={classes.total}>\n        <span>Total Amount</span>\n        <span>{totalAmount}</span>\n      </div>\n      {isCheckout ? (\n        <Checkout onConfirm={submitOrderHandler} onCancel={props.onClose} />\n      ) : (\n        modalActions\n      )}\n    </React.Fragment>\n  );\n\n  const isSubmittingModalConstant = <p>Sending order data...</p>;\n\n  const didSubmitModalContent = (\n    <React.Fragment>\n      <p>Successfully sent the order!</p>\n      <div className={classes.actions}>\n        <button className={classes.button} onClick={props.onClose}>\n          Close\n        </button>\n      </div>\n    </React.Fragment>\n  );\n\n  return (\n    <Modal onClose={props.onClose}>\n      {!isSubmitting && !didSumbit && cartModalContent}\n      {isSubmitting && isSubmittingModalConstant}\n      {!isSubmitting && didSumbit && didSubmitModalContent}\n    </Modal>\n  );\n};\n\nexport default Cart;\n","/Volumes/EXT_DRIVE_2TB_1/UCT/DevSoc/Hackaholics/src/components/Meals/Meals.js",[],"/Volumes/EXT_DRIVE_2TB_1/UCT/DevSoc/Hackaholics/src/store/cart-context.js",[],"/Volumes/EXT_DRIVE_2TB_1/UCT/DevSoc/Hackaholics/src/components/Layout/HeaderCartButton.js",[],"/Volumes/EXT_DRIVE_2TB_1/UCT/DevSoc/Hackaholics/src/components/Cart/Checkout.js",[],"/Volumes/EXT_DRIVE_2TB_1/UCT/DevSoc/Hackaholics/src/components/Meals/MealsSummary.js",[],"/Volumes/EXT_DRIVE_2TB_1/UCT/DevSoc/Hackaholics/src/components/Meals/AvailableMeals.js",[],"/Volumes/EXT_DRIVE_2TB_1/UCT/DevSoc/Hackaholics/src/components/Cart/CartItem.js",[],"/Volumes/EXT_DRIVE_2TB_1/UCT/DevSoc/Hackaholics/src/components/UI/Modal.js",[],"/Volumes/EXT_DRIVE_2TB_1/UCT/DevSoc/Hackaholics/src/components/Cart/CartIcon.js",[],"/Volumes/EXT_DRIVE_2TB_1/UCT/DevSoc/Hackaholics/src/components/UI/Card.js",[],"/Volumes/EXT_DRIVE_2TB_1/UCT/DevSoc/Hackaholics/src/components/Meals/MealItem/MealItem.js",[],"/Volumes/EXT_DRIVE_2TB_1/UCT/DevSoc/Hackaholics/src/components/Meals/MealItem/MealItemForm.js",[],"/Volumes/EXT_DRIVE_2TB_1/UCT/DevSoc/Hackaholics/src/components/UI/Input.js",[],{"ruleId":"156","replacedBy":"157"},{"ruleId":"158","replacedBy":"159"},{"ruleId":"160","severity":1,"message":"161","line":34,"column":11,"nodeType":"162","messageId":"163","endLine":34,"endColumn":19},{"ruleId":"156","replacedBy":"164"},{"ruleId":"158","replacedBy":"165"},{"ruleId":"160","severity":1,"message":"161","line":34,"column":11,"nodeType":"162","messageId":"163","endLine":34,"endColumn":19},"no-native-reassign",["166"],"no-negated-in-lhs",["167"],"no-unused-vars","'response' is assigned a value but never used.","Identifier","unusedVar",["166"],["167"],"no-global-assign","no-unsafe-negation"]